(function(){
  /**
   * 为保证兼容性
   * !不要使用ES6语法
   * !对象最后一个属性后不要加多余的逗号，IE低版本会报错
   */
  console.log("浏览器兼容测试组件");

  //将图片以base64编码直接放入代码文件中
  var sorryBase64 = "data:image/png;base64,";
  var androidBase64 = "data:image/png;base64,";

  var iosBase64 = "data:image/png;base64,";

  var browserVersion = getBrowserVersion();
  var currentBrowser = browserVersion.browser;
  var currentVersion = browserVersion.version;
  var message = getVersionMessage(currentBrowser,currentVersion);

  if(message){

    document.body.style.height = "100%";
    document.body.style.width = "100%";
    document.body.style.overflow = "hidden";
    document.body.style.position = "absolute";

    document.body.appendChild(creatPromptDom({
      message: message,
      title: "向日葵",
      summary: "是1888年8月—1889年1月期间荷兰画家文森特·梵高所绘制的以插在瓶中的向日葵为主要内容的一系列油画作品，作品分别绘制了插在花瓶中的3朵，5朵，12朵，以及15朵向日葵。并分别收藏于德国慕尼黑新美术馆、英国伦敦国家画廊、日本东京兴亜美术馆、荷兰阿姆斯特丹梵高博物馆、美国费城美术馆。",
      bgImageUrl: "https://pic.allhistory.com/T1_9dvByhT1RCvBVdK.jpg?h=600",
      browser: currentBrowser,
      iosQRCode:iosBase64,//ios下载二维码图片
      androidQRCode:androidBase64//android 二维码图片
    }));
  }
  else {
    if(window) {
      window.browserMsg = browserVersion;
    }
  }


  /**
 *  检测浏览器内核版本
 * @returns {Object} browser 浏览器Name（套壳浏览器返回chrome或IE） version 浏览器版本号 string
 */
  function getBrowserVersion(){
    var appVersion = navigator.appVersion, userAgent = navigator.userAgent;
    var result = {
      browser: null,
      version: null
    }
  
    //IE 或者ie套壳浏览器 //! IE的某些版本 appVersion 与 userAgent 中没有“Trident”字段,因此需要配合MSIE
    var regLikeIE2 =  /Trident/img;
    var regLikeIE1 = /MSIE/img;
    //FF
    var regFirefox = /Firefox/img;
    //webkit 内核
    var regWebkit = /AppleWebKit/img;
    //Chrome 或 chrome 套壳浏览器
    var regChrome = /Chrome/img;
    //Safari
    var regSafari = /Version/img;
    //winWeChat
    var regWinWechat = /WindowsWechat/img;
    //mac os
    var regMac = /Mac/img;
  
    var regFirefoxVersion = /Firefox\/([0-9.]+)/img;
    var regChromeVersion  = /Chrome\/([0-9.]+)/img;
    var regSafariVersion = /Version\/([0-9.]+)/img;
    if(regWinWechat.test(appVersion)){
        //windows微信内置浏览器
        result.browser = "WinWechat";
        result.version = regChromeVersion.exec(appVersion)[1];
    }else if(regMac.test(appVersion) && !regChrome.test(appVersion)){
      //mac 微信内置浏览器 //TODO mac下微信判断逻辑需要更加精准，以免误伤
      result.browser = "MacWechat";
    }else if(regFirefox.test(userAgent)){
      //火狐
      result.browser = "Firefox";
      result.version = regFirefoxVersion.exec(userAgent)[1];
    }else if(regWebkit.test(appVersion) && regChrome.test(appVersion)){
      //chrome 或 chrome套壳 (含Opera 与 Edge)
      result.browser = "Chrome";
      result.version = regChromeVersion.exec(appVersion)[1];
    }else if(regWebkit.test(appVersion) && regSafari.test(appVersion)){
      //Safari
      result.browser = "Safari";
      result.version = regSafariVersion.exec(appVersion)[1];
    }else if(regLikeIE1.test(appVersion) || regLikeIE2.test(appVersion)){
      //IE
      result.browser = "IE";
    }
  
    return result;
  }

  /**
 * 判断浏览器版本是否低于限制
 * 版本低于限制返回true 
 */
  function isLowVersion(browser, version){
    console.log(browser,version);
    //此处定义浏览器的最低版本限制
    var limit = {
      Chrome: 51,
      Firefox: 55,
      Safari: 10
    }

    version = parseInt(version);

    if(version < limit[browser]){
      return true;
    }else{
      return false;
    }
  }

  /**
 * 根据浏览器名与版本返回提示的文案，不需要提示时返回false
 */
  function getVersionMessage(browser,version){
    if(browser == "IE"){
      return '您当前的浏览器为IE内核，为保证您的浏览与编辑体验，请切换至极(超)速模式或下载<a href="https://www.google.com/chrome/" title="谷歌浏览器">Chrome</a>以获取更佳体验';
    }else if(browser == "WinWechat"){
      return '请用系统默认浏览器（推荐使用<a href="https://www.google.com/chrome/" title="谷歌浏览器">Chrome</a>浏览器）打开此页面';
    }else if(isLowVersion(browser,version)){
      return '您当前浏览器版本较低，为保证您的浏览与编辑体验，建议升级在新版浏览器中操作';
    }else{
      return false;
    }
  }

  /**
   * 返回拼接好的要插入的浮层div
   * @param {Object} obj 
   *{
      message: "为保证您的浏览与编辑体验，建议您在最新版Chrome浏览器中打开！",
      title: "美丽的费隆妮叶夫人",
      summary: "画作17世纪时的标题“La belle ferronnière”显示，所绘者可能是一名铁匠（即ferronnière）的女儿或妻子。",
      bgImageUrl: "https://pic.allhistory.com/T1fFbTBmdT1RCvBVdK.jpeg?w=1503&h=748"
      browser: "",
      iosQRCode:"",//ios下载二维码图片地址
      androidQRCode:""//android 二维码图片地址
  }
   */
  function creatPromptDom(obj){
    //css 在 fe/src/common/less/prompt-page.less
    var promptInnerHTML = '<div class="error-box">'

      //左侧画作dom
      + '<div class="left-painting" style="background:url(' + obj.bgImageUrl + ');background-position: 50% 50%;"><div class="detail-container"><div class="title">' + obj.title + '</div><div class="summary" title="'+ obj.summary +'">' + obj.summary
      + '</div></div></div>'

      //右侧dom（包含提示与二维码）
      + '<div class="right-message">' 
      //提示信息
      + '<div class="right-top"><div class="content"><img class="sorry" src="' + sorryBase64 + '">'+ obj.message + '</div></div>'

      //二维码
      + '<div class="right-bottom"><div class="app-downloads">' 
      + '<div class="title"><span class="text-or">或</span><span class="text-download">扫码下载</span><span class="text-app">全历史APP</span></div>' 
      + '<div class="ios" title="扫描二维码下载全历史iOS安装包"><div class="qr-code"><img src=" ' + obj.iosQRCode + '" alt="iOS安装包"></div><span class="qr-title">iOS安装包</span></div>' 
      + '<div class="android" title="扫描二维码下载全历史Android安装包"><div class="qr-code"><img src="' + obj.androidQRCode + '" alt="Android安装包"></div><span class="qr-title">Android安装包</span></div>'
      + '</div></div>'
      
      + '</div>'//右侧dom结束
      
      + '</div>';//error-box结束

      if(obj.browser == "WinWechat" || obj.browser == "MacWechat"){
        var className = obj.browser == "MacWechat" ? "mac-wechat" : "";
        promptInnerHTML = promptInnerHTML
          + '<div class="wechat-box' + className + '"><div class="vertical-line"></div><div class="message-box">'
          + obj.message 
          + '</div></div>';
      }

      var dom = document.createElement("div");
      dom.id = "prompt-container";
      dom.innerHTML = promptInnerHTML;

      return dom;
  }

})();